{
  "name": "trackshows-api",
  "version": "2.1.0",
  "private": true,
  "description": "Trackshows API",
  "license": "MIT",
  "author": "Igor Leahu <igor.leahu24@gmail.com>",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0 --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "cli:dev": "ts-node -r tsconfig-paths/register src/cli/index.ts",
    "cli": "node dist/cli/index.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.6",
    "@nestjs/common": "8.4.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "8.4.0",
    "@nestjs/graphql": "^10.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "8.4.0",
    "@prisma/client": "^3.10.0",
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.4",
    "axios": "^0.26.1",
    "axios-retry": "^3.2.4",
    "bcrypt": "^5.0.1",
    "class-validator": "0.13.2",
    "commander": "^9.0.0",
    "googleapis": "^98.0.0",
    "graphql": "^16.3.0",
    "graphql-tools": "8.2.0",
    "nest-commander": "^2.4.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rambda": "^7.0.3",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@eclass/semantic-release-docker": "^3.0.0",
    "@nestjs/cli": "8.2.2",
    "@nestjs/schematics": "8.0.8",
    "@nestjs/testing": "8.4.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^7.0.4",
    "jest": "27.5.1",
    "lint-staged": "^12.3.5",
    "prettier": "2.5.1",
    "prisma": "^3.10.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.13.0",
    "typescript": "4.6.2"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --max-warnings 0 --fix",
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "bash ./scripts/cli.sh seed"
  }
}
